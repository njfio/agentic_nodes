{
  "name": "multimodal-ai-agent",
  "version": "1.0.0",
  "description": "A multimodal AI agent with node-based workflow editor",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:css:prod && nodemon server.js",
    "dev:css": "npx tailwindcss -i ./client/src/styles/tailwind.css -o ./client/dist/styles.css --watch",
    "start:test": "NODE_ENV=test node server.js",
    "build:css": "npx tailwindcss -i ./client/src/styles/tailwind.css -o ./client/dist/styles.css --watch",
    "build:css:prod": "npx tailwindcss -i ./client/src/styles/tailwind.css -o ./client/dist/styles.css --minify",
    "prebuild": "npm run build:css:prod",
    "test": "jest",
    "test:unit": "jest --selectProjects=client,server",
    "test:integration": "jest tests/server/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "coverage:report": "open coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "benchmark": "node tests/performance/benchmark.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "db:check": "node utils/db-check.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/njfio/blackbox-test.git"
  },
  "keywords": [
    "ai",
    "multimodal",
    "node-editor",
    "workflow"
  ],
  "author": "Nicholas Ferguson",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.27.2",
    "@eslint/js": "^9.27.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17"
  }
}
