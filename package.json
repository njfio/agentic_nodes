{
  "name": "multimodal-ai-agent",
  "version": "1.0.0",
  "description": "A multimodal AI agent with node-based workflow editor",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --test tests/health.test.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "db:check": "node utils/db-check.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/njfio/blackbox-test.git"
  },
  "keywords": [
    "ai",
    "multimodal",
    "node-editor",
    "workflow"
  ],
  "author": "Nicholas Ferguson",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
